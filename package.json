  "devDependencies": {
    "@tailwindcss/vite": "^4.2.1",
    "@tanstack/react-router-devtools": "1.162.8",
    "@tanstack/router-plugin": "1.162.8",
    "@types/node": "25.3.0",
    "@types/react": "19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "tailwindcss": "^4.2.1",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-tsconfig-paths": "6.1.1",
    "vitest": "4.0.18"
  }
